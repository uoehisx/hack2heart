{
    "name": "hack2heart",
    "displayName": "hack2heart",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.102.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:hack2heart.test-command",
        "onView:hack2heart.sidebar-welcome"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "hack2heart-sidebar",
                    "title": "Hack2Heart",
                    "icon": "src/assets/logo/logo.png"
                }
            ]
        },
        "views": {
            "hack2heart-sidebar": [
                {
                    "id": "hack2heart.sidebar-welcome",
                    "name": "Welcome"
                }
            ]
        },
        "commands": [
            {
                "command": "hack2heart.test-command",
                "title": "Hack2Heart: Welcome 화면 열기"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.10",
        "@types/node": "20.x",
        "@types/react-dom": "^19.1.6",
        "@types/vscode": "^1.102.0",
        "@typescript-eslint/eslint-plugin": "^8.31.1",
        "@typescript-eslint/parser": "^8.31.1",
        "@vscode/test-cli": "^0.0.11",
        "@vscode/test-electron": "^2.5.2",
        "css-loader": "^7.1.2",
        "eslint": "^9.25.1",
        "mini-css-extract-plugin": "^2.x.x",
        "ts-loader": "^9.5.2",
        "typescript": "^5.8.3",
        "webpack": "^5.99.7",
        "webpack-cli": "^6.0.1"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@types/react": "^19.1.8",
        "react": "^19.1.0",
        "react-dom": "^19.1.0"
    }
}