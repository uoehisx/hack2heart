{
    "name": "hack2heart",
    "displayName": "hack2heart",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.102.0"
    },
    "categories": [
        "Other"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "hack2heart-sidebar",
                    "title": "Hack2Heart",
                    "icon": "src/assets/logo/logo.png"
                }
            ],
            "panel": [
                {
                    "id": "hack2heart-panel",
                    "title": "Hack2Heart Panel",
                    "icon": "$(heart)"
                }
            ]
        },
        "views": {
            "hack2heart-panel": [
                {
                    "type": "webview",
                    "id": "hack2heart.panel-test",
                    "name": "Test"
                },
                {
                    "type": "webview",
                    "id": "hack2heart.panel-explore",
                    "name": "Explore"
                },
                {
                    "type": "webview",
                    "id": "hack2heart.panel-upload",
                    "name": "Upload"
                },
                {
                    "type": "webview",
                    "id": "hack2heart.panel-mycode",
                    "name": "My Code"
                }
            ],
            "hack2heart-sidebar": [
                {
                    "type": "webview",
                    "id": "hack2heart.sidebar-welcome",
                    "name": "Welcome",
                    "icon": "$(home)"
                }
            ]
        },
        "commands": [
            {
                "command": "hack2heart.test-command",
                "title": "Hack2Heart: 테스트 화면 열기"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "webpack",
        "watch": "webpack --watch",
        "package": "webpack --mode production --devtool hidden-source-map",
        "compile-tests": "tsc -p . --outDir out",
        "watch-tests": "tsc -p . -w --outDir out",
        "pretest": "npm run compile-tests && npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.10",
        "@types/node": "20.x",
        "@types/react-dom": "^19.1.6",
        "@types/react-slick": "^0.23.13",
        "@types/vscode": "^1.102.0",
        "@typescript-eslint/eslint-plugin": "^8.31.1",
        "@typescript-eslint/parser": "^8.31.1",
        "@vscode/test-cli": "^0.0.11",
        "@vscode/test-electron": "^2.5.2",
        "css-loader": "^7.1.2",
        "eslint": "^9.25.1",
        "mini-css-extract-plugin": "^2.x.x",
        "socket.io-client": "^4.8.1",
        "ts-loader": "^9.5.2",
        "typescript": "^5.8.3",
        "webpack": "^5.99.7",
        "webpack-cli": "^6.0.1"
    },
    "dependencies": {
        "@dnd-kit/accessibility": "^3.1.1",
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/sortable": "^10.0.0",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@types/react": "^19.1.8",
        "@types/styled-components": "^5.1.34",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-slick": "^0.30.3",
        "slick-carousel": "^1.8.1",
        "socket.io": "^4.8.1",
        "styled-components": "^6.1.19"
    }
}
